@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Http;
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor;
@{
    var CartId = HttpContextAccessor.HttpContext.Session.GetString("CartId");
    var CartMoney = HttpContextAccessor.HttpContext.Session.GetString("CartMoney");
    var CartQty = HttpContextAccessor.HttpContext.Session.GetString("CartQty");
    var CusId = HttpContextAccessor.HttpContext.Session.GetString("CusId");
    var CusName = HttpContextAccessor.HttpContext.Session.GetString("CusName");
    var StfId = HttpContextAccessor.HttpContext.Session.GetString("StfId");
    var StfName = HttpContextAccessor.HttpContext.Session.GetString("StfName");
    var Duty = HttpContextAccessor.HttpContext.Session.GetString("DutyId");
  

}
<!DOCTYPE html>
<html lang="en" class="">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShopMusicProject</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShopMusicProject.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/jquery.datetimepicker.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=NTR&family=Poppins&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Karantina:wght@300;400;700&display=swap" rel="stylesheet">
</head>

<body style="background-color:#111111;">
    <header class="">
        @if (HttpContextAccessor.HttpContext.Session.GetString("StfId") == null) {
            <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow" style="background-color :#282424;height:60px;width :99.5vw;margin-left:3px;margin-right:3px;border-radius:5px;">
                <div class="container-fluid" style="padding-left : 0; padding-right : 0;">
                @*<a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">
                    <i class="bi bi-music-note-beamed"></i>ShopMusicProject
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>*@

                <div class="logo-shop d-flex">
                   @* <div class="d-inline-flex" style="background-color : white; width:25px;"></div>*@
                       @* <img class="h-25" src="/image/logo.png" style="width : 6.5vw;height:72px;"/>*@
                        <svg style="width:90px;height:90px;margin-top:10px;" viewBox="0 0 236 162" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect y="-27" width="236" height="189" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_111_43" transform="matrix(0.00179161 0 0 0.00223714 -0.000753612 0)" />
                                </pattern>
                                <image id="image0_111_43" width="559" height="447" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>

                       @* <div class="d-inline-flex" style="background-color : white;width : 25px;"></div>*@
                    </div>

                
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between" style="padding-left : 20px;">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">หน้รแรก</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="home" asp-action="shop">ร้านค้า</a>@ViewBag.Test
                        </li>
                    </ul>
                    <div class="text-end text-center">@*CusId : @CusName  [@CartId : @CartMoney : @CartQty]*@
                            <a class="nav-link text-white pe-2" asp-area="" asp-controller="cart" asp-action="list" asp-route-cusid="@CusId">
                                <svg width="39" height="34" viewBox="0 0 39 34" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M12.2969 26.5806C11.1827 26.5806 10.2778 27.3695 10.2778 28.3407C10.2778 29.312 11.1827 30.1023 12.2969 30.1023C13.4095 30.1023 14.3145 29.312 14.3145 28.3407C14.3145 27.3695 13.4095 26.5806 12.2969 26.5806Z" fill="white" />
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M29.5204 26.5806C28.4062 26.5806 27.5012 27.3695 27.5012 28.3407C27.5012 29.312 28.4062 30.1023 29.5204 30.1023C30.6346 30.1023 31.5395 29.312 31.5395 28.3407C31.5395 27.3695 30.6346 26.5806 29.5204 26.5806Z" fill="white" />
                                    <path fill-rule="evenodd" clip-rule="evenodd" d="M21.7601 16.1258H28.3691V14.0534H21.7601V16.1258ZM9.98424 8.65146L9.50877 4.42383H3.32764V6.4962H7.3533L9.38197 24.0422H32.8703L35.2001 8.65146H9.98424Z" fill="white" />
                                </svg>

                            </a>
                    </div> <div class="text-end pe-2">
                        <ul class="navbar-nav ms-auto mb-2 mb-lg-0 profile-menu " style="width : 220px; height:auto;">
                            <li class="nav-item dropdown ">
                                    @{
                                        var imgcus = "/imgcus/" + CusId + ".jpg";
                                    }
                                <a class="nav-link dropdown-toggle ps-1 pt-2 " href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    @if (HttpContextAccessor.HttpContext.Session.GetString("CusId") !=null)
                                    {
                                        <div class="profile-pic">
                                            <img src="@imgcus" alt="Profile Picture">
                                               
                                        </div>
                                            <span class="text-white">@CusName</span>
                                    }else{
                                        
                                        <i class="fas fa-user pt-3 ps-5" style="height:50px; width:50px;"></i>
                                        <span class="text-white ps-4">ผู้ใช้งาน</span>
                                       
                                    }
                                   
                                </a>
                                
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown" style="width:220px;">
                                        @if (HttpContextAccessor.HttpContext.Session.GetString("CusId") != null)
                                        {
                                            <li><a class="dropdown-item" asp-area="" asp-controller="home" asp-action="show" asp-route-id="@CusId"><i class="fas fa-sliders-h fa-fw"></i>บัญชี</a></li>
                                            <li><hr class="dropdown-divider"></li>
                                            <li><a class="dropdown-item" asp-action="logout" asp-controller="home"><i class="fas fa-sign-out-alt fa-fw"></i>ออกสู่ระบบ</a></li>
                                        }
                                        else
                                        {
                                                 <li> <a class="dropdown-item" asp-area="" asp-controller="home" asp-action="login"><i class="bi bi-person-circle fa-fw"></i>เข้าสู่่ระบบ</a></li>
                                        <li><a class="dropdown-item " asp-area="" asp-controller="home" asp-action="register"><i class="bi bi-pencil-fill fa-fw"></i>สมัครสมาชิก</a></li>
                                        }
                                </ul>
                            </li>
                        </ul>
                </div>
            </div>
        </nav>
        }
    </header>
    <div class="d-flex">
        @if (HttpContextAccessor.HttpContext.Session.GetString("StfId") != null)
        {
            <div class="bg-dark" style="height :900px;width:350px; font-size:15px;">
                <ul class="ps-0" style="list-style:none;">
                    <li class="p-2"><img class="" style="width:220px;height:150px" src="~/image//logo.png"/></li>
                    <li class="m-1 ps-1 pe-1"><h5 class="text-white text-decoration-none">@Duty : @StfName</h5></li>
                    @if (HttpContextAccessor.HttpContext.Session.GetString("DutyId") == "Admin")
                    {
                        <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1 " asp-action="list" asp-controller="employee">จัดการพนักงาน</a></li>
                    }
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="index" asp-controller="product">จัดการสินค้า</a></li>
                   @* <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="" asp-controller="">จัดการประเภทสินค้าและหมวดหมู่</a></li>*@
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="Index" asp-controller="Buying">จัดการซื้อสินค้า</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="JournalPurchase" asp-controller="report">รายงานซื้อ</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="JournalSale" asp-controller="report">รายงานขาย</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="JournalStock" asp-controller="report">รายงานตัดstock</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="SaleDaily" asp-controller="report">ยอดขายสินค้ารายวัน</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="saleMonthly" asp-controller="report">ยอดขายสินค้ารายเดือน</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="SumPurchaseDaily" asp-controller="report">ยอดซื้อสินค้ารายวันตามลูกค้า</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="SumPurchaseMonthly" asp-controller="report">ยอดซื้อสินค้ารายเดือนตามลูกค้า</a></li>
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="WorkForEmployee" asp-controller="report">รายการเข้างานของพนักงาน</a></li>
                    @*<li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="DashBoard" asp-controller="Employee">หน้ารายงานสรุป</a></li>*@
                    <li class="p-1 list-admin"><a class="text-white text-decoration-none ps-1" asp-action="logout" asp-controller="Employee">ออกจากระบบ</a></li>
                </ul>
				
				
            </div>
        }
       
        <main role="main" class="w-100 ">
            @RenderBody()
        </main>
    </div>
    <style>
        /* Profile Picture */
        .profile-pic {
            display: inline-block;
           vertical-align: middle;
            width: 50px;
            height: 50px;
            overflow: hidden;
            border-radius: 50%;
        }

            .profile-pic img {
                width: 100%;
                height: auto;
                object-fit: cover;
            }

        .profile-menu .dropdown-menu {
            right: unset;
            left: 0;
        }

        .profile-menu .fa-fw {
            margin-right: 10px;
        }

        .toggle-change::after {
            border-top: 0;
            border-bottom: 0.3em solid;
        }
    </style>
 
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/jquery.datetimepicker.full.min.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
